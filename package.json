{
  "name": "avicena",
  "version": "1.0.0",
  "description": "Avicena",
  "author": "Perdiz",
  "license": "MIT",
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:fix && npm run eslint && npm run eslint:client",
    "eslint:client": "cd client && npm run lint",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "pretest": "npm run clean && npm run build",
    "test": "lb-mocha --allow-console-logs \"dist/test\"",
    "posttest": "npm run lint",
    "test:dev": "lb-mocha --allow-console-logs dist/test/**/*.js && npm run posttest",
    "docker:build": "docker build -t avicena .",
    "docker:run": "docker run -p 3000:3000 -d avicena",
    "prestart": "npm run build",
    "serve": "npm run start & cd client && npm run serve",
    "start": "node -r source-map-support/register .",
    "cli": "node dist/scripts",
    "prepublishOnly": "npm run test"
  },
  "dependencies": {
    "@loopback/authentication": "^3.2.3",
    "@loopback/boot": "^1.5.12",
    "@loopback/context": "^1.24.0",
    "@loopback/core": "^1.11.0",
    "@loopback/openapi-v3": "^1.10.2",
    "@loopback/repository": "^1.15.5",
    "@loopback/rest": "^1.24.0",
    "@loopback/rest-explorer": "^1.4.5",
    "@loopback/service-proxy": "^1.3.12",
    "bcryptjs": "^2.4.3",
    "connect-history-api-fallback": "^1.6.0",
    "dotenv": "^8.2.0",
    "ejs": "^3.0.1",
    "express": "^4.17.1",
    "inquirer": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "loopback-connector-postgresql": "^3.8.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.4.2"
  },
  "devDependencies": {
    "@loopback/build": "^2.1.0",
    "@loopback/eslint-config": "^4.1.5",
    "@loopback/testlab": "^1.9.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/connect-history-api-fallback": "^1.3.3",
    "@types/ejs": "^3.0.0",
    "@types/express": "^4.17.2",
    "@types/inquirer": "^6.5.0",
    "@types/multer": "^1.3.10",
    "@types/node": "^10.17.5",
    "@types/nodemailer": "^6.4.0",
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/parser": "^2.9.0",
    "eslint": "^6.7.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-eslint-plugin": "^2.1.0",
    "eslint-plugin-mocha": "^6.2.2",
    "source-map-support": "^0.5.16",
    "typescript": "~3.7.2"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/test"
  ]
}
